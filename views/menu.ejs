<!DOCTYPE html>
<html lang="en">
<head>
  <meta charset="UTF-8">
  <meta http-equiv="X-UA-Compatible" content="IE=edge">
  <meta name="viewport" content="width=device-width, initial-scale=1.0">
  <title>Document</title>
</head>
<body>
  <h1>List Food Page</h1>
  <form action="/menu/add">
    <button><a role="button">Buy Some Food</a></button>
  </form>
  <table border="1">
    <tr>
      <th>menu</th>
      <th>stock</th>
      <th>harga</th>
      <th>Action</th>
    </tr>
    <% for ( let i = 0; i < menu.length; i++ ) { %> 
      <tr>
        <td> <%= menu[i].name %> </td>
        <td> <%= menu[i].stock %> </td>
        <td> <%= menu[i].harga %> </td>
        <td> detail </td>
      </tr>
    <% } %>
  </table>
</body>
</html>